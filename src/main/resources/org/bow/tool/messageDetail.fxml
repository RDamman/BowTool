<VBox prefHeight="595.0" prefWidth="660.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.bow.tool.MessageDetailController">
  <children>
    <MenuBar prefHeight="27.0" prefWidth="725.0" VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Action">
          <items>
            <MenuItem mnemonicParsing="false" text="Clear" />
            <MenuItem fx:id="menuitemAdd" mnemonicParsing="false" text="Add">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="F2" control="UP" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
            <MenuItem fx:id="menuitemUpdate" mnemonicParsing="false" text="Update" />
            <SeparatorMenuItem mnemonicParsing="false" />
            <MenuItem fx:id="menuitemWindowClose" mnemonicParsing="false" text="Close">
                     <accelerator>
                        <KeyCodeCombination alt="UP" code="F4" control="DOWN" meta="UP" shift="UP" shortcut="UP" />
                     </accelerator></MenuItem>
          </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="574.0" prefWidth="660.0" VBox.vgrow="ALWAYS">
