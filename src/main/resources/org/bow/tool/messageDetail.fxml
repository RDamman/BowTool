                        <ComboBox fx:id="comboBoxCommand" layoutX="5.0" layoutY="68.0" prefHeight="25.0" prefWidth="251.0" promptText="Command (Byte)" />
                        <ComboBox fx:id="comboBoxData" layoutX="5.0" layoutY="97.0" prefHeight="25.0" prefWidth="251.0" promptText="Data (Byte)" />
                     </children>
                  </AnchorPane>
                  <Label layoutX="14.0" layoutY="-9.0" style="-fx-background-color: white;" text="BowBus Message" />
                  <TextField fx:id="textfieldPayload" layoutX="9.0" layoutY="264.0" prefHeight="25.0" prefWidth="260.0" />
                  <Label layoutX="10.0" layoutY="247.0" text="Payload / Extra Message Data (Hex)">
                     <padding>
                        <Insets left="5.0" right="5.0" />
                     </padding>
                  </Label>
                  <CheckBox fx:id="checkBoxAddCRC8" layoutX="13.0" layoutY="300.0" mnemonicParsing="false" text="Add Calculated CRC8" />
                  <TextField fx:id="textfieldCRC8" layoutX="157.0" layoutY="295.0" prefHeight="27.0" prefWidth="35.0" style="-fx-border-color: gray; -fx-background-color: #f0f0f0;" />
                  <Label layoutX="9.0" layoutY="328.0" text="Message Raw">
                     <padding>
                        <Insets left="5.0" right="5.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="textfieldMessageRaw" layoutX="11.0" layoutY="345.0" prefHeight="27.0" prefWidth="260.0" style="-fx-border-color: gray; -fx-background-color: #f0f0f0;" />
                  <Label layoutX="9.0" layoutY="379.0" text="Decoded">
                     <padding>
                        <Insets left="5.0" right="5.0" />
                     </padding>
                  </Label>
                  <TextArea fx:id="textareaMessageDecoded" layoutX="11.0" layoutY="396.0" prefHeight="69.0" prefWidth="260.0" style="-fx-control-inner-background: #f0f0f0; -fx-border-color: grey;" />
               </children>
            </AnchorPane>
            <TextArea fx:id="textareaComment" layoutX="315.0" layoutY="76.0" prefHeight="63.0" prefWidth="328.0" style="-fx-background-color: white; -fx-border-color: grey;" />
            <Label layoutX="314.0" layoutY="58.0" text="Comment">
               <padding>
                  <Insets left="5.0" right="5.0" />
               </padding>
            </Label>
            <Button fx:id="buttonUpdate" layoutX="519.0" layoutY="528.0" mnemonicParsing="false" prefHeight="27.0" prefWidth="124.0" text="Update" />
            <TextField fx:id="textfieldTimestamp" layoutX="315.0" layoutY="36.0" prefHeight="25.0" prefWidth="124.0" />
            <Label layoutX="315.0" layoutY="19.0" text="Timestamp">
               <padding>
                  <Insets left="5.0" right="5.0" />
               </padding>
            </Label>
      </children>
    </AnchorPane>
  </children>
</VBox>
