                     <padding>
                        <Insets left="5.0" right="5.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="textfieldConvertString" layoutX="14.0" layoutY="221.0" prefHeight="25.0" prefWidth="301.0" />
